      "text": "import psycopg2\nimport psycopg2.extras\nimport nltk\nfrom nltk.tokenize import regexp_tokenize\n\nimport re\n\nfrom elasticsearch import Elasticsearch\n\nes \u003d Elasticsearch([\"localhost\"], port\u003d9200)\n\ndef saveES(id, src, ent, content):\n    body \u003d {\n      \u0027name\u0027: ent,\n#      \u0027aname\u0027: [\u0027test1\u0027, \u0027joy1\u0027, \u0027fun1\u0027],\n      \u0027source\u0027: src, \n#      \u0027content\u0027: content,\n      \u0027tags\u0027: [\u0027literature\u0027]\n    }\n#    print(body)\n    es.index(index\u003d\u0027infobot\u0027, doc_type\u003d\u0027v1\u0027, id\u003did, body\u003dbody)\n    \nconn \u003d psycopg2.connect(database\u003d\"notes\", user\u003d\"postgres\", password\u003d\"post\", host\u003d\"127.0.0.1\", port\u003d\"5432\")\n\n#cur \u003d conn.cursor()\ncur \u003d conn.cursor(cursor_factory\u003dpsycopg2.extras.DictCursor)\nsql \u003d \"\"\"select * from infobot\n        where id like %s\"\"\"\n\nbooks \u003d[]\ndataSrc \u003d None\ncur.execute(sql, (\"《本草纲目》_04_%\",))\nrows \u003d cur.fetchall()\nfor x in rows:\n    print(x[\u0027title\u0027])\n    dataSrc \u003d x[\"id\"]\n    #book names in title\n    temp \u003d re.sub(u\u0027《本草纲目》_\u0027, \u0027\u0027, x[\u0027title\u0027])\n    t \u003d regexp_tokenize(temp, u\"《(.*?)》\")    #example line: \u003cb0 《神农本草经》\n    #print(t)\n    #for b in t:\n    #    print(b)\n    #book names in content \n    c \u003d regexp_tokenize(x[\u0027content\u0027], u\"《(.*?)》\")  #extract from content lines\n    #print(c)\n    books \u003d books + t + c\n    #for b in c:\n    #    print(b)\n\n#dedup\nbooks \u003d list(dict.fromkeys(books))\nfor b in books:\n    saveES(dataSrc+b, dataSrc, b, None)\n    print(b)\n\ncur.close()\nconn.close()",
