int(treatment)\n                neo4jSess.write_transaction(save_neo4j, name+\u0027.\u0027+abnorm, \u0027ILLNESS\u0027, abnorm, \u0027TREATING\u0027, name+\u0027.\u0027+abnorm + \u0027.\u0027+ treatment, abnorm, src, name+\u0027.\u0027+abnorm + \u0027.\u0027+ treatment, \u0027METHOD\u0027, treatment)\n                o \u003d re.split(\u0027,\u0027, n[1])\n                for p in o:\n                    print(name, abnorm, treatment, p.strip(), src)\n                    neo4jSess.write_transaction(save_neo4j, name+\u0027.\u0027+abnorm + \u0027.\u0027+ treatment, \u0027METHOD\u0027, treatment, \u0027EXAMPLE\u0027, \u0027example\u0027, \u0027example\u0027, src, p.strip(), \u0027HERB\u0027, p.strip())   \n    neo4jSess.close()\n    driver.close()\n    \nconn \u003d psycopg2.connect(database\u003d\"notes\", user\u003d\"postgres\", password\u003d\"post\", host\u003d\"127.0.0.1\", port\u003d\"5432\")\n\n#cur \u003d conn.cursor()\ncur \u003d conn.cursor(cursor_factory\u003dpsycopg2.extras.DictCursor)\nsql \u003d \"\"\"select * from infobot\n        where title like \u0027%脏腑虚实标本用药式%\u0027 order by id\"\"\"   #of 04prefix_1.txt\n\ncur.execute(sql)\nrows \u003d cur.fetchall()\nfor x in rows:\n    name \u003d re.sub(u\u0027《本草纲目》.*_\u0027, \u0027\u0027, x[\u0027title\u0027])\n    #print(name)\n    temp \u003d parseChapter(name, x[\u0027id\u0027], x[\u0027content\u0027])\n\ncur.close()\nconn.close()",
