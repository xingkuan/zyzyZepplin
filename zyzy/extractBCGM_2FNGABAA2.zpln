      "text": "import nltk\nimport psycopg2.extras\nfrom nltk.tokenize import regexp_tokenize\nimport re\n\nfrom neo4j import GraphDatabase\n\nuri \u003d \"bolt://localhost:7687\"\n\ndef save_neo4jMethod(tx, ent1, cat1, src, ent2, cat2):\n    cql \u003d \"\"\"\n    merge (en1:Entity{name: $en1, cat:$cat1})\n    merge (en2:Entity{name: $en2, cat:$cat2})\n    merge (en1)-[r:吐法{src:$src}]-\u003e(en2)\n    \"\"\"\n    result \u003d tx.run(cql, en1\u003dent1, cat1\u003dcat1, src\u003dsrc, en2\u003dent2, cat2\u003dcat2)\n    return result\n\ndef save_neo4jEnA(tx, ent1, cat1, rel, src, ent2, cat2):\n    cql \u003d \"\"\"\n    merge (en1:Entity{name: $en1, cat:$cat1})\n    merge (en2:Entity{name: $en2, cat:$cat2})\n    merge (en1)-[r:ATTRI{name: $rel, src:$src}]-\u003e(en2)\n    \"\"\"\n    result \u003d tx.run(cql, en1\u003dent1, cat1\u003dcat1, rel\u003drel, src\u003dsrc, en2\u003dent2, cat2\u003dcat2)\n    return result\n\n\ndef parseLine(src, str):\n    #str \u003d u\u0027辛苦而寒者，常山、藜芦、郁金；甘而寒者，桐油。\u0027\n    temp \u003d re.search(\u0027^(.*)者，(.*)(；|。|$)\u0027, str)\n    if temp:\n        print(src)\n        print(temp.group(1), temp.group(2))\n        driver \u003d GraphDatabase.driver(uri, auth\u003d(\"neo4j\", \"lisa\"))\n        neo4jSess \u003d driver.session()\n        weis \u003d re.findall(\u0027([辛苦甘酸咸])\u0027, temp.group(1))\n        qis \u003d re.findall(\u0027([寒温平热])\u0027, temp.group(1))\n        print(weis)\n        print(qis)\n        herbs \u003d re.split(\u0027、|。\u0027, temp.group(2).strip())\n        print(herbs)\n        for h in herbs:\n            print(h, \u0027qi\u0027, qis)\n            print(h, \u0027wei\u0027, weis)\n            print(h, \u0027吐\u0027)\n            neo4jSess.write_transaction(save_neo4jMethod, \u0027痰饮宿食\u0027, \u0027problem\u0027, src, h, \u0027herb\u0027)\n            if weis:\n                print(h, \u0027wei\u0027, weis)\n                for w in weis:\n                    neo4jSess.write_transaction(save_neo4jEnA, h, \u0