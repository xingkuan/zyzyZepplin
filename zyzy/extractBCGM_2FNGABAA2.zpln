      "text": "import nltk\nimport psycopg2.extras\nfrom nltk.tokenize import regexp_tokenize\nimport re\n\nfrom neo4j import GraphDatabase\n\nuri \u003d \"bolt://localhost:7687\"\n\ndef save_neo4j(tx, ent1, cat1, dName1, rel, rName, rdName, src, ent2, cat2, dName2):\n    cql \u003d \"\"\"\n    merge (en1:\u003ccat1\u003e{name: $en1}) set en1.dName\u003d$dName1 \n    merge (en2:\u003ccat2\u003e{name: $en2}) set en2.dName\u003d$dName2 \n    merge (en1)-[r:\u003crel\u003e{name: $rName}]-\u003e(en2) set r.dName\u003d$rdName, r.src\u003dr.src + \u0027.\u0027 +$src\n    \"\"\"\n    cql \u003d re.sub(\u0027\u003ccat1\u003e\u0027, cat1, cql)\n    cql \u003d re.sub(\u0027\u003ccat2\u003e\u0027, cat2, cql)\n    cql \u003d re.sub(\u0027\u003crel\u003e\u0027, rel, cql)\n    print(cql)\n    result \u003d tx.run(cql, en1\u003dent1, dName1\u003ddName1, rName\u003drName, rdName\u003drdName, src\u003dsrc, en2\u003dent2, dName2\u003ddName2)\n\ndef parseLine(src, str):\n    driver \u003d GraphDatabase.driver(uri, auth\u003d(\"neo4j\", \"lisa\"))\n    neo4jSess \u003d driver.session()\n\n    temp \u003d re.findall(\u0027(.*?)之邪，\u0027, str)\n    for t in temp:\n        print(t)\n        neo4jSess.write_transaction(save_neo4j, t, \u0027ILLNESS\u0027, t, \u0027TREATING\u0027,  \u0027汗法\u0027, \u0027汗法\u0027, src, \u0027汗法\u0027, \u0027METHOD\u0027, \u0027汗法\u0027)\n        \n    #str \u003d u\u0027荆芥、薄荷、白芷、陈皮、半夏、细辛、苍术、天麻、生姜、葱白，皆辛而温者也。\u0027\n    # \u003d\u003d\u003e (\u0027荆芥、薄荷、白芷、陈皮、半夏、细辛、苍术、天麻、生姜、葱白\u0027, \u0027皆辛而温者也\u0027)\n    temp \u003d re.search(\u0027^(.*)，(.*)$\u0027, str)\n    if temp:\n        #print(src)\n        #print(temp.group(1), temp.group(2))\n        weis \u003d re.findall(\u0027([辛苦甘酸咸])\u0027, temp.group(2))\n        qis \u003d re.findall(\u0027([寒温平热])\u0027, temp.group(2))\n        #print(weis)\n        #print(qis)\n        herbs \u003d re.split(\u0027、|。\u0027, temp.group(1).strip())\n        for h in herbs:\n            print(h, \u0027汗法\u0027)\n            neo4jSess.write_transaction(save_neo4j, \u0027汗法\u0027, \u0027METHOD\u0027, \u0027汗法\u0027, \u0027EXAMPLE\u0027, \u0027example\u0027, \u0027use\u0027, src, h, \u0027HERB\u0027, h)\n            if weis:\n                print(h, \u0027wei\u0027, weis)\n                for w in weis:\n                    if w !\u003d \u0027\u0027:\n                        neo4jSess.write_transaction(save_neo4j, h, \u0027HERB\u0027, h, \u0027ATTRI\u0027, \u0027HAS\u0027, \u0027has\u0027, src, w, \u0027味\u0027, w)\n            if qis:\n                print(h, \u0027qi\u0027, qis)\n                for q in qis:\n                    if q !\u003d \u0027\u0027:\n                        neo4jSess.write_transaction(save_neo4j, h, \u0027HERB\u0027, h, \u0027ATTRI\u0027, \u0027HAS\u0027, \u0027has\u0027, src, q, \u0027气\u0027, q)\n    neo4jSess.close()\n    driver.close()\n    return None\n\nconn \u003d psycopg2.connect(database\u003d\"notes\", user\u003d\"postgres\", password\u003d\"post\", host\u003d\"127.0.0.1\", port\u003d\"5432\")\n\n#cur \u003d conn.cursor()\ncur \u003d conn.cursor(cursor_factory\u003dpsycopg2.extras.DictCursor)\nsql \u003d \"\"\"select * from infobot\n        where title like \u0027%汗法%\u0027\"\"\"\n\n#### 02 ####\ncur.execute(sql)\nrows \u003d cur.fetchall()\nfor x in rows:\n    name \u003d re.sub(u\u0027《本草纲目》.*_\u0027, \u0027\u0027, x[\u0027title\u0027])\n    #print(name)\n    lines \u003d x[\u0027content\u0027].splitlines()\n    src \u003d x[\u0027id\u0027]\n    for l in lines:\n        temp \u003d parseLine(src, l)\n\ncur.close()\nconn.close()\n\n",
