      "text": "import psycopg2\nimport psycopg2.extras\nimport nltk\nfrom nltk.tokenize import regexp_tokenize\nimport re\n\nfrom neo4j import GraphDatabase\n\nfrom elasticsearch import Elasticsearch\n\nes \u003d Elasticsearch([\"localhost\"], port\u003d9200)\n\ndef save_es(i, s, t, c):\n    body \u003d {\n      \u0027name\u0027: t,\n#      \u0027aname\u0027: [\u0027test1\u0027, \u0027joy1\u0027, \u0027fun1\u0027],\n      \u0027source\u0027: s, \n      \u0027content\u0027: c,\n      \u0027tags\u0027: [\u0027李东垣\u0027, \u0027用药\u0027]\n    }\n#    print(body)\n    es.index(index\u003d\u0027infobot\u0027, doc_type\u003d\u0027v1\u0027, id\u003di, body\u003dbody)\n    \n\nuri \u003d \"bolt://localhost:7687\"\n\ndef save_neo4j(tx, ent1, desc1, src, ent2, desc2):\n    cql \u003d \"\"\"\n    merge (en1:Entity{name: $en1, cat:\"illness\", desc:$desc1})\n    merge (en2:Entity{name: $en2, cat:\"herb\", desc:$desc2})\n    merge (en1)-[r:李东垣{src: $src}]-\u003e(en2)  \n    \"\"\"\n    result \u003d tx.run(cql, en1\u003dent1, desc1\u003ddesc1, src\u003dsrc, en2\u003dent2, desc2\u003ddesc2)\n    return result\n\ndef parseLine(src, problem, str):\n    #print(problem, str)\n    driver \u003d GraphDatabase.driver(uri, auth\u003d(\"neo4j\", \"lisa\"))\n    neo4jSess \u003d driver.session()\n    temp \u003d re.search(\u0027^(.*):(.*)[，。；]\u0027, str)  #先发其表: 羌活、防风为君，随证加药\n    if temp:\n        desc1 \u003d temp.group(1)\n        temp1 \u003d re.findall(\u0027(.*?)(为君|为臣|佐)\u0027, temp.group(2))\n        if temp1:\n            effect \u003d re.sub(\u0027为\u0027, \u0027\u0027, temp1[0][1])\n            herbs \u003d re.split(\u0027、\u0027, temp1[0][0])\n            for h in herbs:\n                print(desc1, effect, re.sub(\u0027^加\u0027, \u0027\u0027, h))   #temp1[0][1]: 君|为臣|佐\n                neo4jSess.write_transaction(save_neo4j, problem, desc1, src, re.sub(\u0027^加\u0027, \u0027\u0027, h), effect)\n                                # 风中六腑*手足不遂, 先发其表, src, 羌活, 君  \n            return\n        temp1 \u003d re.split(\u0027、|。\u0027, temp.group(2))\n        if(len(temp1) \u003e 0):\n            for i in temp1:\n                if i !\u003d \u0027\u0027:\n                    z \u003d re.split(\u0027之|随\u0027, i)   #独活之类，随经用之 \u003d\u003e 独活  \n                    print(desc1, re.sub(\u0027^加|用\u0027, \u0027\u0027, z[0]))\n                    neo4jSess.write_transaction(save_neo4j, problem, desc1, src, z[0], \u0027\u0027)\n            return\n    #temp \u003d re.search(\u0027^(.*)为君，(.*)(.*)佐之\u0027, str) #防风为君，麻黄、甘草佐之。 \n                                                      #防风、芩、连泻火，当归佐。酒煎服。\n                                                      #熟地、当归为君，羌、防为臣，甘草、甘菊之类佐之。\n    temp \u003d re.findall(\u0027(.*?)(为君|为臣|为使|为之使|佐|泻.火)\u0027, str)     #防风、芩、连泻火，当归佐。酒煎服。\n    if temp:                                                #  ....黄芩泻肺火\n        #print(temp)\n        for t in temp:\n            effect \u003d re.sub(\u0027为|之\u0027, \u0027\u0027, t[1])\n            herbs \u003d re.split(\u0027、|，\u0027, t[0])\n            for h in herbs:\n                if h !\u003d \u0027\u0027:\n                    print( effect, re.sub(\u0027之|类|为|须|用\u0027, \u0027\u0027, h))\n                    neo4jSess.write_transaction(save_neo4j, problem, \u0027\u0027, src, h, effect)\n        return\n    temp \u003d re.search(\u0027须[用|加](.*?)[，|。]\u0027, str)  #须用川芎。加引经药∶\n    if temp:                             #须加当归，详上下，用根梢。\n        herbs \u003d re.split(\u0027、\u0027, temp.group(1))\n        for h in herbs:\n            print(re.sub(\u0027须|用|加|为\u0027, \u0027\u0027, h))\n            neo4jSess.write_transaction(save_neo4j, problem, \u0027\u0027, src, h, \u0027\u0027)\n        return\n    temp \u003d re.sub(\u0027，.*$\u0027, \u0027\u0027, str)      #黄芩、鼠粘子、甘草、桔梗。\n    temp \u003d re.split(\u0027、|。\u0027, temp)   #，xxx , eg: ，加引经药。 \n    if(len(temp) \u003e 1):              #\n        for h in temp:              \n            if h !\u003d \u0027\u0027:\n                print(h)\n                neo4jSess.write_transaction(save_neo4j, problem, \u0027\u0027, src, h, \u0027\u0027)\n        return\n    \n    neo4jSess.close()\n    driver.close()\n    return None        \n\n\nconn \u003d psycopg2.connect(database\u003d\"notes\", user\u003d\"postgres\", password\u003d\"post\", host\u003d\"127.0.0.1\", port\u003d\"5432\")\n\n#cur \u003d conn.cursor()\ncur \u003d conn.cursor(cursor_factory\u003dpsycopg2.extras.DictCursor)\nsql \u003d \"\"\"select * from infobot\n        where title like \u0027%李东垣随证用药凡例%\u0027\"\"\"\n\ncur.execute(sql)\nrows \u003d cur.fetchall()\nfor x in rows:\n    print(x[\u0027title\u0027])\n    segs \u003d re.findall(\u0027(.*?)（(.*?)）\u0027, x[\u0027content\u0027], re.DOTALL)\n    for s in segs:\n        problem \u003d s[0].strip()\n        print(\u0027-\u0027+problem)\n        lines \u003d s[1].strip().splitlines()\n        for l in lines:\n            temp \u003d parseLine(x[\u0027id\u0027], problem, l)\n\ncur.close()\nconn.close()",
