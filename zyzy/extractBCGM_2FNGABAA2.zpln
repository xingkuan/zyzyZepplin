      "text": "import nltk\nimport psycopg2.extras\nfrom nltk.tokenize import regexp_tokenize\nimport re\n\nfrom neo4j import GraphDatabase\n\nuri \u003d \"bolt://localhost:7687\"\n\ndef save_neo4j(tx, ent1, rel, src, ent2):\n    print(rel)\n    cql \u003d \"\"\"\n    merge (en1:Entity{name: $en1, cat:\u0027herb\u0027})\n    merge (en2:Entity{name: $en2, cat:\u0027herb\u0027})\n    merge (en1)-[r:COMPATILE{name: $rel, src:$src}]-\u003e(en2)\n    \"\"\"\n    result \u003d tx.run(cql, en1\u003dent1, rel\u003drel, src\u003dsrc, en2\u003dent2)\n    return result\n\ndef construnctRelationTuples(es1, r, src, es2):\n    driver \u003d GraphDatabase.driver(uri, auth\u003d(\"neo4j\", \"lisa\"))\n    neo4jSess \u003d driver.session() \n\n    es1\u003d es1.split( \u0027、\u0027)\n    es2 \u003d es2.split( \u0027、\u0027)\n    \n    for i in es1:\n        for j in es2:\n            print( (i, r, j))\n            neo4jSess.write_transaction(save_neo4j, i, r, src, j)    \n    neo4jSess.close()\n    driver.close()\n\ndef parseCompatible(src, str):\n    #str \u003d u\u0027甘草（术、苦参、干漆为之使。恶远志。忌猪肉。）\u0027\n    #print(str)\n    ents1 \u003d regexp_tokenize(str, u\"(^.*)（\")\n    temp \u003d regexp_tokenize(str, u\"（(.*)）\")\n    temp \u003d temp[0].split(\"。\")\n    for s in temp:\n        #print(s)\n        agents \u003d regexp_tokenize(s, u\"(.*)为之使\")\n        if agents:\n            rslt \u003d construnctRelationTuples(ents1[0],\u0027使\u0027, src, agents[0] )\n            return rslt\n\n        nice \u003d regexp_tokenize(s, u\"得(.*)良\")\n        if nice:\n            rslt \u003d construnctRelationTuples(ents1[0],\"良\", src, nice[0] )\n            return rslt\n\n        rel \u003d regexp_tokenize(s, u\"([恶,忌,畏,伏,杀,制])(.*)\")\n        if rel:\n            rslt \u003d construnctRelationTuples(ents1[0], rel[0][0], src, rel[0][1] )\n            return rslt\n            \n    return None\n\nconn \u003d psycopg2.connect(database\u003d\"notes\", user\u003d\"postgres\", password\u003d\"post\", host\u003d\"127.0.0.1\", port\u003d\"5432\")\n\n#cur \u003d conn.cursor()\ncur \u003d conn.cursor(cursor_factory\u003dpsycopg2.extras.DictCursor)\nsql \u003d \"\"\"select * from infobot\n        where title like \u0027%使畏忌伏良%\u0027\"\"\"\n\n#### 02 ####\ncur.execute(sql)\nrows \u003d cur.fetchall()\nfor x in rows:\n    name \u003d re.sub(u\u0027《本草纲目》.*_\u0027, \u0027\u0027, x[\u0027title\u0027])\n    #print(name)\n    lines \u003d x[\u0027content\u0027].splitlines()\n    src \u003d x[\u0027id\u0027]\n    for l in lines:\n        if re.search(u\u0027[使,畏,忌,伏,良]\u0027, l):\n            #print(l)\n            temp \u003d parseCompatible(src, l)\n            #if temp:\n            #    compatibleList.append(temp)\n\n#for i in compatibleList:\n#    print(i)\n\ncur.close()\nconn.close()",
