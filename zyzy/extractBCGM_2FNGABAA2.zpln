      "text": "import codecs\nimport re\nimport psycopg2\nimport psycopg2.extras\n\nimport random\nimport string\n\nfrom neo4j import GraphDatabase\n\nfrom elasticsearch import Elasticsearch\n\nes \u003d Elasticsearch([\"localhost\"], port\u003d9200)\ndef save_es(id, src, name, content):\n    body \u003d {\n      \u0027name\u0027: name,\n#      \u0027aname\u0027: [\u0027test1\u0027, \u0027joy1\u0027, \u0027fun1\u0027],\n      \u0027source\u0027: src, \n      \u0027content\u0027: content,\n      \u0027tags\u0027: [\u0027list-of\u0027]\n    }\n#    print(body)\n    es.index(index\u003d\u0027infobot\u0027, doc_type\u003d\u0027v1\u0027, id\u003did, body\u003dbody)\n\nuri \u003d \"bolt://localhost:7687\"\ndef save_neo4j(tx, ent1, cat1, dName1, rel, rName, rdName, src, ent2, cat2, dName2):\n    cql \u003d \"\"\"\n    merge (en1:\u003ccat1\u003e{name: $en1}) set en1.dName\u003d$dName1 \n    merge (en2:\u003ccat2\u003e{name: $en2}) set en2.dName\u003d$dName2 \n    merge (en1)-[r:\u003crel\u003e{name: $rName}]-\u003e(en2) set r.dName\u003d$rdName, r.src\u003dr.src + \u0027.\u0027 +$src\n    \"\"\"\n    cql \u003d re.sub(\u0027\u003ccat1\u003e\u0027, cat1, cql)\n    cql \u003d re.sub(\u0027\u003ccat2\u003e\u0027, cat2, cql)\n    cql \u003d re.sub(\u0027\u003crel\u003e\u0027, rel, cql)\n    print(cql)\n    result \u003d tx.run(cql, en1\u003dent1, dName1\u003ddName1, rName\u003drName, rdName\u003drdName, src\u003dsrc, en2\u003dent2, dName2\u003ddName2)\n\ndef parseSeg(title, src, context):\n    driver \u003d GraphDatabase.driver(uri, auth\u003d(\"neo4j\", \"lisa\"))\n    neo4jSess \u003d driver.session()\n    \n    temp \u003d re.sub(u\u0027[【】]\u0027, \u0027\u0027, title) #《本草纲目》_诸风_【风寒风湿】\u003d\u003e 诸风, 风寒风湿\n    temp \u003d temp.split(\u0027_\u0027)\n    parent \u003d temp[1]\n    child \u003d temp[2]\n    print(\u0027-\u0027, parent, child)\n    neo4jSess.write_transaction(save_neo4j, parent, \u0027ILLNESS\u0027, parent, \u0027CHILD\u0027, \u0027CHILD\u0027, \u0027child\u0027, src, parent+\u0027.\u0027+child, \u0027ILLNESS\u0027, child)    \n\n    lines \u003d content.splitlines()\n    for l in lines:\n        temp \u003d re.findall(\u0027(.*)（(.*)）\u0027, l)  #车前子,水蓼,陆英,飞廉,忍冬,坐拿草,蒴,伏牛花,石南藤,百灵藤（酒。\n        if temp:\n            ext \u003d temp[0][1]\n            herbs \u003d re.split(\u0027,\u0027, temp[0][0])\n            for h in herbs:\n                print(h, ext)\n                neo4jSess.write_transaction(save_neo4j, parent+\u0027.\u0027+child, \u0027ILLNESS\u0027, child, \u0027EXAMPLE\u0027, \u0027example\u0027, \u0027use\u0027, src, h, \u0027HERB\u0027, h)\n                save_es(h+\u0027: \u0027+parent+\u0027.\u0027+child, src, h+\u0027: \u0027+parent+\u0027.\u0027+child, ext)\n    \n    neo4jSess.close()\n    driver.close()\n\n# infobots are to be stored in a database, to be furtuer parsed into finer infobot    \nconn \u003d psycopg2.connect(database\u003d\"notes\", user\u003d\"postgres\", password\u003d\"post\", host\u003d\"127.0.0.1\", port\u003d\"5432\")\n#print(\"Database opened successfully\")\n#cur \u003d conn.cursor()\ncur \u003d conn.cursor(cursor_factory\u003dpsycopg2.extras.DictCursor)\nsql \u003d \"\"\"select * from infobot\n        where tags like \u0027%%hypothesis%%\u0027 \n        and id ~ \u0027《本草纲目》_0[67]\u0027\n        order by id\"\"\"\n\n\nrsltList \u003d []\n#### 0 ####\ncur.execute(sql)\nrows \u003d cur.fetchall()\nfor x in rows:\n    title \u003d x[\u0027title\u0027]\n    src \u003d x[\u0027id\u0027]\n    content \u003d x[\u0027content\u0027]\n    parseSeg(title, src, content)\n\nconn.commit()\ncur.close()\nconn.close()",
