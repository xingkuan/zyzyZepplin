      "text": "import psycopg2\nimport psycopg2.extras\nimport nltk\nfrom nltk.tokenize import regexp_tokenize\nimport re\n\nfrom neo4j import GraphDatabase\n\nuri \u003d \"bolt://localhost:7687\"\n\ndef save_neo4j(tx,ent1, cat1, dName1, rel, rName, src, ent2, cat2, dName2):\n    cql \u003d \"\"\"\n    merge (en1:\u003ccat1\u003e{name: $en1}) set en1.disPlayName\u003d$dName1 \n    merge (en2:\u003ccat2\u003e{name: $en2}) set en2.disPlayName\u003d$dName2  \n    merge (en1)-[r:\u003crel\u003e{name: $rName}]-\u003e(en2) set r.src\u003dr.src + \u0027|\u0027+$src\n    \"\"\"\n    cql \u003d re.sub(\u0027\u003ccat1\u003e\u0027, cat1, cql)\n    cql \u003d re.sub(\u0027\u003ccat2\u003e\u0027, cat2, cql)\n    cql \u003d re.sub(\u0027\u003crel\u003e\u0027, rel, cql)\n    print(cql)\n    result \u003d tx.run(cql, en1\u003dent1,cat1\u003dcat1, dName1\u003ddName1, rName\u003drName, src\u003dsrc, en2\u003dent2,cat2\u003dcat2, dName2\u003ddName2)\n    #return result\n\ndef parseSeg(src, subsys, str):\n    driver \u003d GraphDatabase.driver(uri, auth\u003d(\"neo4j\", \"lisa\"))\n    neo4jSess \u003d driver.session() \n    lines \u003d str.splitlines()\n    for l in lines:\n        #print(subsys, l)\n        #苦急，急食甘以缓之（甘草），以酸泻之（赤芍药），实则泻子（甘草）。\n        #欲散，急食辛以散之（川芎），以辛补之（细辛），虚则补母    . That is, pattern \"（甘草）\" is optional\n        #temp \u003d re.findall(\u0027(.*?)，急食(.)以(.)之(（.*?）)?，[用以](.)(.)之(（.*?）)?，(.)则(.)(.)(（.*?）)?\u0027, l)\n        temp \u003d re.search(\u0027(.*?)，(.*)$| i\u0027, l)\n        subProb \u003d temp.group(1)\n        neo4jSess.write_transaction(save_neo4j, subsys, \u0027脏\u0027, subsys, \u0027SOMEHOW\u0027, \u0027somehow\u0027, src, subsys+\u0027.\u0027+subProb,\u0027SYMPTOM\u0027, subProb)\n        solutions \u003d re.findall(\u0027食(.)以(.)之(（.*?）)?\u0027, temp.group(2))  #急食甘以缓之（甘草）\n        for s in solutions:\n            print(len(s))\n            wei \u003d s[0]\n            effect \u003d s[1]\n            neo4jSess.write_transaction(save_neo4j, subsys+\u0027.\u0027+subProb, \u0027SYMPTOM\u0027, subProb, \u0027EAT\u0027, wei, src, subsys+\u0027.\u0027+subProb+\u0027.\u0027+effect, \u0027EFFECT\u0027, effect)\n            herbs \u003d re.split(\u0027、\u0027, re.sub(\u0027[（）]\u0027, \u0027\u0027, s[2]))\n            if herbs:\n                for h in herbs:\n                    print(subsys, subsys+\u0027.\u0027+subProb, wei, effect, h)\n                    if h.strip() !\u003d \u0027\u0027:\n                        neo4jSess.write_transaction(save_neo4j, subsys+\u0027.\u0027+subProb+\u0027.\u0027+effect, \u0027EFFECT\u0027, effect, \u0027EXAMPLE\u0027, \u0027example\u0027, src, h, \u0027HERB\u0027, h)\n            else:\n                print(subsys, subsys+\u0027.\u0027+subProb, wei, effect)\n        solutions \u003d re.findall(\u0027[以|用|，](.)(.)之(（.*?）)?\u0027, temp.group(2))  #以酸泻之（赤芍药）\n        for s in solutions:\n            print(len(s))\n            #print(s)\n            wei \u003d s[0]\n            effect \u003d s[1]\n            neo4jSess.write_transaction(save_neo4j, subsys+\u0027.\u0027+subProb, \u0027SYMPTOM\u0027, subProb, \u0027EAT\u0027, wei, src, subsys+\u0027.\u0027+subProb+\u0027.\u0027+effect, \u0027EFFECT\u0027, effect)\n            if s[2]\u003d\u003d\u0027\u0027:\n                print(subsys, subsys+\u0027.\u0027+subProb, wei, effect)\n            else:\n                herbs \u003d re.split(\u0027、\u0027, re.sub(\u0027[（）]\u0027, \u0027\u0027, s[2]))\n                for h in herbs:\n                    if h !\u003d \u0027\u0027:\n                        print(subsys, subsys+\u0027.\u0027+subProb, wei, effect, h)\n                        neo4jSess.write_transaction(save_neo4j, subsys+\u0027.\u0027+subProb+\u0027.\u0027+effect, \u0027EFFECT\u0027, effect, \u0027EXAMPLE\u0027, \u0027example\u0027, src, h, \u0027HERB\u0027, h)\n        solutions \u003d re.findall(\u0027(.)则(.)(.)(（.*?）)?\u0027, temp.group(2))  #实则泻子（甘草）\n        for s in solutions:\n            subP1 \u003d s[0]\n            print(len(s))\n            method \u003d s[1]\n            target \u003d s[2]\n            neo4jSess.write_transaction(save_neo4j, subsys+\u0027.\u0027+subProb, \u0027SYMPTOM\u0027, subProb, \u0027CHILD\u0027, subP1, src, subsys+\u0027.\u0027+subProb+\u0027.\u0027+subP1, \u0027SYMPTOM\u0027, subP1)\n            neo4jSess.write_transaction(save_neo4j, subsys+\u0027.\u0027+subProb+\u0027.\u0027+subP1, \u0027SYMPTOM\u0027, subP1, \u0027ACT\u0027, method, src, subsys+\u0027.\u0027+subProb+\u0027.\u0027+target, \u0027METHOD\u0027, target)\n            herbs \u003d re.split(\u0027、\u0027, re.sub(\u0027[（）]\u0027, \u0027\u0027, s[3]))\n            for h in herbs:\n                print(\u0027herbs:\u0027, h)\n                if h.strip() !\u003d \u0027\u0027:\n                    print(h)\n                    neo4jSess.write_transaction(save_neo4j, subsys+\u0027.\u0027+subProb+\u0027.\u0027+target, \u0027METHOD\u0027, target, \u0027EXAMPLE\u0027, \u0027example\u0027, target, h, \u0027HERB\u0027, h)\n\n\n    neo4jSess.close()\n    driver.close()\n    return None        \n\n\nconn \u003d psycopg2.connect(database\u003d\"notes\", user\u003d\"postgres\", password\u003d\"post\", host\u003d\"127.0.0.1\", port\u003d\"5432\")\n\n#cur \u003d conn.cursor()\ncur \u003d conn.cursor(cursor_factory\u003dpsycopg2.extras.DictCursor)\nsql \u003d \"\"\"select * from infobot\n        where title like \u0027%五脏五味补泻%\u0027\"\"\"\n\ncur.execute(sql)\nrows \u003d cur.fetchall()\nfor x in rows:\n    print(x[\u0027title\u0027])\n    src \u003d x[\u0027id\u0027]\n    segs \u003d re.findall(\u0027([肝心脾肺肾])\\n(.*?\\n.*?)\\n\\n\u0027, x[\u0027content\u0027], re.DOTALL)\n    for s in segs:\n        subsys \u003d s[0]\n        parseSeg(src, subsys, s[1])\n\ncur.close()\nconn.close()",
