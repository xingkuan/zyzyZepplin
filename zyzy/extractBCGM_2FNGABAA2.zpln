      "text": "import psycopg2\nimport psycopg2.extras\nimport nltk\nfrom nltk.tokenize import regexp_tokenize\nimport re\n\nfrom neo4j import GraphDatabase\n\nuri \u003d \"bolt://localhost:7687\"\ndef save_neo4j(tx, ent1, cat1, dName1, rel, rName, rdName, src, ent2, cat2, dName2):\n    cql \u003d \"\"\"\n    merge (en1:\u003ccat1\u003e{name: $en1}) set en1.dName\u003d$dName1 \n    merge (en2:\u003ccat2\u003e{name: $en2}) set en2.dName\u003d$dName2 \n    merge (en1)-[r:\u003crel\u003e{name: $rName}]-\u003e(en2) set r.dName\u003d$rdName, r.src\u003dr.src + \u0027.\u0027 +$src\n    \"\"\"\n    cql \u003d re.sub(\u0027\u003ccat1\u003e\u0027, cat1, cql)\n    cql \u003d re.sub(\u0027\u003ccat2\u003e\u0027, cat2, cql)\n    cql \u003d re.sub(\u0027\u003crel\u003e\u0027, rel, cql)\n    print(cql)\n    result \u003d tx.run(cql, en1\u003dent1, dName1\u003ddName1, rName\u003drName, rdName\u003drdName, src\u003dsrc, en2\u003dent2, dName2\u003ddName2)\n\nse\u003dNone    \ndef parseLine(src, str):\n    global se\n    driver \u003d GraphDatabase.driver(uri, auth\u003d(\"neo4j\", \"lisa\"))\n    neo4jSess \u003d driver.session()\n    #水生咸。\n    #辛i散，\n    #青色宜酸，\n    #肝病宜食麻、犬、李、韭。\n    m \u003d re.match(\u0027^(.*)(欲|生|is|色宜|宜食)(.*)\u0027,str)\n    if m :    \n        itemA \u003d m.group(1)\n        rel \u003d  m.group(2)\n        itemB \u003d re.sub(\u0027，|。\u0027, \u0027\u0027, m.group(3))\n        print(itemA, rel, itemB)\n        if (rel\u003d\u003d\u0027生\u0027):\n            neo4jSess.write_transaction(save_neo4j,itemA,\u0027五行\u0027,itemA, \u0027RELATIONSHIP\u0027,rel,rel,src, itemB,\u0027味\u0027,itemB)\n        if (rel\u003d\u003d\u0027is\u0027):\n            neo4jSess.write_transaction(save_neo4j, itemA,\u0027味\u0027,itemA, \u0027RELATIONSHIP\u0027,\u0027主\u0027,\u0027主\u0027,src, itemB,\u0027EFFECT\u0027,itemB)\n        if (rel\u003d\u003d\u0027欲\u0027):\n            neo4jSess.write_transaction(save_neo4j, itemA,\u0027脏\u0027,itemA, \u0027RELATIONSHIP\u0027,rel,rel,src, itemB,\u0027味\u0027,itemB)\n        if (rel\u003d\u003d\u0027色宜\u0027):\n            se \u003d itemA  # for parsing next line\n            neo4jSess.write_transaction(save_neo4j, itemA,\u0027色\u0027,itemA, \u0027RELATIONSHIP\u0027,\u0027宜\u0027,\u0027宜\u0027,src, itemB,\u0027味\u0027,itemB)\n        if (rel\u003d\u003d\u0027宜食\u0027):    #青色宜酸，\n            print(\u0027宜食\u0027)    #肝病宜食麻、犬、李、韭。\n            #neo4jSess.write_transaction(save_neo4j, itemA,\u0027脏\u0027,itemA, \u0027RELATIONSHIP\u0027,\u0027宜\u0027,\u0027宜\u0027,src, itemB,\u0027味\u0027,itemB)\n            #  that should be deducted from DB. \n            #Let\u0027s save them as examples:\n            herbs \u003d re.split(\u0027、\u0027, itemB)\n            for h in herbs:\n                h \u003d re.sub(\u0027。\u0027, \u0027\u0027, h)\n                print(h, se)\n                neo4jSess.write_transaction(save_neo4j, h,\u0027HERB\u0027,h, \u0027ATTRI\u0027,\u0027色\u0027,\u0027色\u0027,src, se,\u0027色\u0027,se)\n                \n    neo4jSess.close()\n    driver.close()\n    return None        \n\n\nconn \u003d psycopg2.connect(database\u003d\"notes\", user\u003d\"postgres\", password\u003d\"post\", host\u003d\"127.0.0.1\", port\u003d\"5432\")\n\n#cur \u003d conn.cursor()\ncur \u003d conn.cursor(cursor_factory\u003dpsycopg2.extras.DictCursor)\nsql \u003d \"\"\"select * from infobot\n        where title like \u0027%五味宜忌%\u0027 \n           or title like \u0027%五欲%\u0027 \n           or title like \u0027%五宜%\u0027 \"\"\"      #04prefix_1.txt\n\ncur.execute(sql)\nrows \u003d cur.fetchall()\nfor x in rows:\n    src \u003d x[\u0027id\u0027]\n\n    lines \u003d x[\u0027content\u0027].splitlines()\n    for l in lines:\n        temp \u003d parseLine(src, l)\n\ncur.close()\nconn.close()",
