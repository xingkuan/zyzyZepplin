      "title": "xChapter 15 and plus",
      "text": "#import codecs\nimport re\nimport psycopg2\nimport psycopg2.extras\n\nimport random\nimport string\n\nrsltList \u003d []\n\ndef parseMxLine(name, cat, str):\n    #print(str)\n    #str \u003d \u0027夫妻各饮一杯，还房，当获时有子，神效。（藏器）\u0027\n    #temp \u003d re.search(u\u0027(^.*。)(（.*）)?\u0027, str)\n    temp \u003d re.split(\u0027[（）]\u0027, str)\n    #print(len(temp))\n    if temp:\n        print((name, cat, temp[0], temp[1] if len(temp)\u003e2 else None))\n\ndef parseExLine(name, cat, str):\n    #print(str)\n    #temp \u003d re.findall(u\u0027^(.*?)∶(.*)（(.*)）\u0027, str)\n    #temp \u003d re.search(u\u0027(.*。)(（.*）)?\u0027, str)\n    #if temp:\n    #    print(temp.groups())\n    temp \u003d re.split(\u0027[∶（）]|$\u0027, str)\n    #print(temp)\n    if temp:\n        print((name, cat, temp[0], temp[1] if len(temp)\u003e1 else None, temp[2] if len(temp)\u003e2 else None))\n\ndef parseParagraph(name, str):\n    print(name)\n    #print(str)\n    #lines \u003d str.splitlines()\n    src \u003d \u0027\u0027\n    nameExpond \u003d\u0027\u0027\n    wei \u003dNone\n    qi \u003d None\n    du \u003d None\n    mainApplications \u003d None\n    moreApplications \u003d None\n    #extract source:\n    temp \u003d re.findall(u\u0027（《(.*?)》）\u0027, str)\n    if temp:\n        src \u003d temp[0]\n        print(\"source:\", src)\n    #extract source:\n    temp \u003d re.findall(u\u0027【释名】(.*?)【\u0027, str, re.DOTALL)\n    if temp:\n        nameExpond \u003d temp[0].strip()\n        print(\u0027释名\u0027, nameExpond)\n    #extract tri characteristics:\n    temp \u003d re.findall(u\u0027【气味】(.*?)。\u0027, str, re.DOTALL)\n    if temp:\n        triCharact \u003d temp[0].split(\u0027，、\u0027)\n        triCharact \u003d re.split(u\"[、,，，]\", temp[0])\n        #print(triCharact)\n        for i in triCharact:\n            tt \u003d i.strip()\n            if re.search(\u0027[咸甘辛苦涩]\u0027, tt):\n                if wei is None:\n                    wei \u003d tt\n                else:\n                    wei \u003d wei + \u0027,\u0027 + tt\n            if re.search(\u0027[平温寒凉冷热滑]\u0027, tt):\n                if qi is None:\n                    qi \u003d tt\n                else:\n                    qi \u003d qi + \u0027,\u0027 + tt\n            #print(tt)\n            if re.search(\u0027[毒]\u0027, tt):\n                if du is None:\n                    du \u003d tt\n                else:\n                    du \u003d du + \u0027,\u0027 + tt\n        print(\"味:\", wei)\n        print(\u0027气: \u0027, qi)\n        print(\"毒:\", du)\n        #print((triCharact[0].strip(), triCharact[1].strip(), triCharact[2].strip()))\n    #extract application:\n    temp \u003d re.findall(u\u0027【主治】(.*?)(\\(|【|$)\u0027, str, re.DOTALL)\n    #print(temp[0][0])\n    if temp:\n        mainApplications \u003d temp[0][0]\n        lines \u003d mainApplications.splitlines()\n        for l in lines:\n            if re.match(\u0027^$\u0027,l):\n                continue\n            parseMxLine(name, \u0027主治\u0027, l)\n\n    #extract further application:\n#    testStr \u003d \"\"\"\n#    和蚌粉，敷暑月痱疮，及腋下赤肿，立瘥（陈承）。\n#【附方】\n#新一。\n#寒热疟疾（秋后霜一钱半，热酒服之。《集玄方》）\n#\n#\n#    \"\"\"\n#    temp \u003d re.findall(u\u0027【附方】(.*?)[【$]\u0027, testStr, re.DOTALL)\n    #temp \u003d re.findall(\u0027【附方】(.*?)$|【\u0027, testStr, re.DOTALL)\n#    print(temp)\n    #somehow, \u0027【附方】(.*?)[$【]\u0027 is not working  !!??    \n    #temp \u003d re.findall(\u0027【附方】(.*?)【\u0027, str, re.DOTALL)\n    #if not temp:\n    #    temp \u003d re.findall(\u0027【附方】(.*?)$\u0027, str, re.DOTALL)\n    temp \u003d re.search(\u0027【附方】(.*?)(【|$)\u0027, str, re.DOTALL)\n    if temp:\n        moreApplications \u003d temp.group(1)\n        lines \u003d moreApplications.splitlines()\n        for l in lines:\n            if re.match(\u0027(^$)|(^[新旧])\u0027,l):\n                continue\n            parseExLine(name, \u0027附方\u0027, l)\n        \n    temp \u003d re.findall(u\u0027【发明】(.*?)(\\(|【|$)\u0027, str, re.DOTALL)\n    #print(temp[0][0])\n    if temp:\n        mainApplications \u003d temp[0][0]\n        lines \u003d mainApplications.splitlines()\n        for l in lines:\n            if re.match(\u0027^$\u0027,l):\n                continue\n            parseMxLine(name, \u0027发明\u0027, l)\n    \n    temp \u003d re.findall(u\u0027【集解】(.*?)(\\(|【|$)\u0027, str, re.DOTALL)\n    #print(temp[0][0])\n    if temp:\n        mainApplications \u003d temp[0][0]\n        lines \u003d mainApplications.splitlines()\n        for l in lines:\n            if re.match(\u0027^$\u0027,l):\n                continue\n            parseMxLine(name, \u0027集解\u0027, l)\n            \n    #m \u003d re.findall(u\u0027(^.*)（(.*）)\u0027,str)\n    #if m:\n    #    t \u003d re.split(u\"[、,，]\", m[0][0])\n    #    for b in t:\n    #        print(b)\n    return None\n\n\n\n# infobots are to be stored in a database, to be furtuer parsed into finer infobot    \nconn \u003d psycopg2.connect(database\u003d\"notes\", user\u003d\"postgres\", password\u003d\"post\", host\u003d\"127.0.0.1\", port\u003d\"5432\")\n#print(\"Database opened successfully\")\n#cur \u003d conn.cursor()\ncur \u003d conn.cursor(cursor_factory\u003dpsycopg2.extras.DictCursor)\nsql \u003d \"\"\"select * from infobot\n        where id \u003e %s\n        order by id\"\"\"\n\n\n#### 0 ####\ncur.execute(sql, (\"《本草纲目》_15_\",))\nrows \u003d cur.fetchall()\nfor x in rows:\n    print(x[\u0027id\u0027])\n    temp \u003d re.sub(u\u0027《本草纲目》_\u0027, \u0027\u0027, x[\u0027title\u0027])\n    #print(type(name))\n    name \u003d re.sub(u\u0027[【】]\u0027, \u0027\u0027, temp)\n    name \u003d re.sub(u\u0027_\u0027, \u0027|\u0027, name)\n\n    temp \u003d parseParagraph(name, x[\u0027content\u0027])\n\nfor i in rsltList:\n    print(i)\n\nconn.commit()\ncur.close()\nconn.close()",
